{
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
    },
    "ecmaVersion": 6,
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-console": "off",
    "prefer-arrow-callback": "error",
    "react/prop-types": 0,
  }
}